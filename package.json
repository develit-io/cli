{
  "name": "develit",
  "version": "1.9.0",
  "description": "Develit.io CLI Tool",
  "author": "Develit.io",
  "license": "ISC",
  "type": "module",
  "bin": {
    "develit": "bin/develit.mjs"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/develit.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "start": "node dist/cli.js",
    "build": "unbuild",
    "prepack": "unbuild",
    "typecheck": "tsc --noEmit",
    "changelogen": "bunx changelogen@latest --bump",
    "release": "bun run build && bunx changelogen@latest --release --push && npm publish --access public"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@types/node": "24.10.1",
    "giget": "^2.0.0",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "citty": "^0.1.6",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1"
  }
}
